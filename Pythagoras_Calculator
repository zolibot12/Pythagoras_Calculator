#include <iostream>
#include <cmath>
#include <cstdlib>

int main(){
    double a;
    double b;
    double c;

    std::cout << "Enter Side A: " << '\n';
    std::cin >> a;

    std::cout << "Enter Side B: " << '\n';
    std::cin >> b;

    double C = sqrt(pow(a, 2) + pow(b, 2));

    std::cout << "Side C Is Equal To: " << C;

    system("pause");

    return 0;
}
